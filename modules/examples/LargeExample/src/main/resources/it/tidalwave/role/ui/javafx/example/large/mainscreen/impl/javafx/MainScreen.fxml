<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TreeTableColumn?>
<?import javafx.scene.control.TreeTableView?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane maxWidth="800.0" minWidth="800.0" prefWidth="800.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.tidalwave.role.ui.javafx.example.large.mainscreen.impl.javafx.JavaFXMainScreenPresentationDelegate">
    <bottom>
        <TextArea fx:id="taLog" prefHeight="112.0" prefWidth="857.0" BorderPane.alignment="CENTER" />
    </bottom>
    <center>
        <VBox spacing="8.0" BorderPane.alignment="CENTER">
            <children>
                <HBox alignment="BASELINE_LEFT" spacing="8.0">
                    <children>
                        <Button fx:id="btButton" mnemonicParsing="false" prefHeight="24.0" text="Button" />
                        <TextField fx:id="tfTextField" prefHeight="24.0" />
                        <CheckBox mnemonicParsing="false" text="CheckBox" />
                    </children>
                </HBox>
            <HBox spacing="8.0">
               <children>
                  <Button fx:id="btDialogOk" mnemonicParsing="false" text="Button" />
                  <Button fx:id="btDialogOkCancel" mnemonicParsing="false" text="Button" />
                  <Button fx:id="btPickFile" mnemonicParsing="false" text="Button" />
                  <Button fx:id="btPickDirectory" mnemonicParsing="false" text="Button" />
               </children>
            </HBox>
                <HBox minHeight="400.0" spacing="8.0">
                    <children>
                        <VBox prefWidth="200.0" spacing="16.0">
                            <children>
                                <ComboBox fx:id="cbComboBox" prefWidth="150.0" />
                                <ChoiceBox prefWidth="150.0" />
                                <ListView fx:id="lvListView" prefHeight="200.0" prefWidth="200.0" />
                            </children>
                        </VBox>
                        <TableView fx:id="tvTableView" prefHeight="200.0" prefWidth="200.0">
                            <columns>
                                <TableColumn prefWidth="75.0" text="C1" />
                                <TableColumn prefWidth="75.0" text="C2" />
                                <TableColumn prefWidth="75.0" text="C3" />
                            </columns>
                        </TableView>
                        <TreeView fx:id="tvTreeView" prefHeight="200.0" prefWidth="200.0" />
                        <TreeTableView fx:id="ttvTreeTableView" prefHeight="200.0" prefWidth="200.0">
                            <columns>
                                <TreeTableColumn prefWidth="75.0" text="C1" />
                                <TreeTableColumn prefWidth="75.0" text="C2" />
                                <TreeTableColumn prefWidth="75.0" text="C3" />
                            </columns>
                        </TreeTableView>
                    </children>
                </HBox>
            </children>
        </VBox>
    </center>
    <padding>
        <Insets bottom="16.0" left="16.0" right="16.0" top="16.0" />
    </padding>
</BorderPane>
